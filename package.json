{
  "name": "partyparty",
  "version": "1.0.0",
  "description": "Get started by customizing your environment (defined in the .idx/dev.nix file) with the tools and IDE extensions you'll need for your project!",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5432/postgres nodemon index.js",
    "migrate": "node-pg-migrate",
    "migrate:local": "cross-env DATABASE_URL=postgres://postgres:postgres@localhost:5432/postgres node-pg-migrate",
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "node-pg-migrate": "^8.0.4",
    "pg": "^8.16.3",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "firebase-tools": "^13.13.0",
    "nodemon": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.4.1",
    "typescript": "^5.7.3"
  }
}
